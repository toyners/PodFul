<Window x:Class="PodFul.WPF.Windows.AddFeedProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PodFul.WPF.Windows"
        mc:Ignorable="d"
        Title="AddFeedFeedback" Height="100" Width="300" ResizeMode="NoResize" Loaded="WindowLoaded">
    <StackPanel>
      <ProgressBar Name="ProgressBar" Margin="0,10,0,5" Width="240" Height="20"/>
      <Button Name="CancelButton" Margin="0,5" Width="60" Click="CancelButtonClick"> Cancel</Button>
  </StackPanel>
</Window>
